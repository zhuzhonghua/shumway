package {	import flash.display.MovieClip;	import flash.geom.Point;	public class Banana extends MovieClip {		public function Banana() {			var g = this.graphics;			for (var x = 0; x < 2; x++) {				for (var y = 0; y < 2; y++) {					var c:Point = new Point(x * 110, y * 110);					c.offset(60, 60);					for (var i = 0; i < 100; i++) {						var color = Math.round(Math.random() * 0xFFFFFF);						g.lineStyle(2, color , 0.75);						g.moveTo(c.x, c.y);						g.lineTo(c.x + Math.cos(i) * 50, c.y + Math.sin(i) * 50);					}				}			}		}	}}